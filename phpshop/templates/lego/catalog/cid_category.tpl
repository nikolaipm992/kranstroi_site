<div class="col-xs-4 col-md-4 col-sm-4 catalog-wrap">
    <a href="/shop/CID_@podcatalogId@.html" title="@podcatalogName@" class="catalog-img" >
        <div class="img">
            <img data-src="@podcatalogIcon@" alt="@podcatalogName@" title="@podcatalogName@">
        </div>
        <div class="catalog-desc">
            <div class="catalog-desc-wrap">
                <div class="catalog-background"></div>
                <div class="catalog-title" style="-webkit-filter: invert(@podcatalogColor@%);filter: invert(@podcatalogColor@%);">@podcatalogName@</div>
            </div>
        </div>
    </a>
</div>