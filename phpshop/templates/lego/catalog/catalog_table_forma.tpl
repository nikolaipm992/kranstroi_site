<div class="col-xs-6 col-md-4 col-sm-4 catalog-wrap">
    <a href="/shop/CID_@catalogId@.html" title="@catalogName@" class="catalog-img" >
        <div class="img">
            <img data-src="@catalogIcon@" alt="@catalogName@" title="@catalogName@">
        </div>
        <div class="catalog-desc">
            <div class="catalog-desc-wrap">
                <div class="catalog-background"></div>
                <div class="catalog-title" style="-webkit-filter: invert(@catalogColor@%);filter: invert(@catalogColor@%);">@catalogName@</div>

            </div>
        </div>
    </a>
</div>