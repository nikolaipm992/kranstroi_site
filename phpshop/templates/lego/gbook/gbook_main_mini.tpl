<div class="miniGbook" itemscope itemtype="https://schema.org/Review">
    <meta itemprop="datePublished" content="@gbookData@"/>
    <a itemprop="url" href="/gbook/ID_@gbookId@.html">
        <div class="gbook-wrap">
            <div class="gbook-body">
                <span itemprop="itemReviewed" itemscope itemtype="https://schema.org/Organization">
                    <span class="h3" itemprop="name">"@gbookTema@"</span>
                </span>
                <span itemprop="reviewBody">@gbookOtsiv@</span>
                
                <footer itemprop="author" itemscope itemtype="https://schema.org/Person"><cite><span itemprop="name">@gbookName@</span>, @gbookData@</cite></footer>
            </div>
        </div>
    </a>
</div>