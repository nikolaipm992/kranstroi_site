<tr>
    <td class="image">           <a href="@shopDir@/shop/UID_@visualcart_product_id@@visualcart_product_seo@.html" title="@visualcart_product_name@">
            <img src="@visualcart_product_pic_small@"  border="0" alt="@visualcart_product_name@">
        </a>
    </td>
    <td class="name"> <a href="@shopDir@/shop/UID_@visualcart_product_id@@visualcart_product_seo@.html" title="@visualcart_product_name@">
            @visualcart_product_name@      </a> 
    </td>
    <td class="quantity">@visualcart_product_num@ шт.</td>
    <td class="total">@visualcart_product_price@ @visualcart_product_currency@</td>
    <td class="remove">  <a title="Удалить" href="javascript:visualCart(@visualcart_product_xid@)">
            <img src="images/shop/cart_delete.gif"  alt="Удалить"  border="0" alt="@visualcart_product_name@">
        </a></td>
</tr>


